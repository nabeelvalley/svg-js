{
  "name": "snake-svg",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently --kill-others \"yarn run parcel-start\" \"yarn run ts-watch\"",
    "parcel-start": "parcel src/index.html",
    "ts-watch": "tsc --watch --noEmit src/index.ts",
    "build": "parcel build src/index.html"
  },
  "dependencies": {
    "parcel-bundler": "^1.12.4",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "concurrently": "^5.1.0"
  }
}
